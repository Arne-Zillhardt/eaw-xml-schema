              <xsd:element type="xsd:string" name="Projectile_Stun_On_Detonation"/>
              <xsd:element type="xsd:short" name="Projectile_Stun_Duration_Frames"/>
              <xsd:element type="xsd:short" name="Projectile_Stun_Radius"/>
