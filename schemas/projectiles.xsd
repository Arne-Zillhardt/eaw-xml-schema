              <xsd:element type="xsd:string" name="Land_Model_Name"/>
              <xsd:element type="xsd:string" name="Projectile_Target_Point_On_Terrain"/>
              <xsd:element type="xsd:string" name="Projectile_Stun_Victims_Unit_Types"/>
